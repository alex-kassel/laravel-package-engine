{
  "name": "alex-kassel/laravel-package-engine",
  "description": "Laravel helper to create/manage local packages under /packages",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "AlexKassel\\LaravelPackageEngine\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "AlexKassel\\LaravelPackageEngine\\LaravelPackageEngineServiceProvider"
      ]
    }
  },
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^10.0|^11.0|^12.0"
  },
  "autoload-dev": {
    "psr-4": {
      "AlexKassel\\LaravelPackageEngine\\Tests\\": "tests/"
    }
  }
}
